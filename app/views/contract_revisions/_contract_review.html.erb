<p class="h4">Reviewed by the following attorneys</p>
<%= render 'shared/fine_print' %>
<table class="table">
  <thead>
    <tr>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Country</th>
      <th scope="col">Last updated</th>
    </tr>
  </thead>
  <tbody>
    <% @user_loggings.each do |u| %>
      <tr>
        <td><%= u.user.first_name %></td>
        <td><%= u.user.last_name %></td>
        <td><%= u.user.country %></td>
        <td><%= u.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
